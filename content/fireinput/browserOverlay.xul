          <menuitem id="fireinputContextSwitchZHToBG" label="" value="0"
                   oncommand="FireinputEncoding.switchToBig5()"/>
          <menuitem id="fireinputContextSwitchBGToZH" label="" value="0"
                   oncommand="FireinputEncoding.switchToZH()"/>
        </menupopup>
      </menu>
</popup>

