<commandset id="mainCommandSet">
	<command id="cmd_focusSearchBox" oncommand="FireinputWebSearch.focusSearchbox();"/>
</commandset>

<keyset id="mainKeyset">
        <key id="key_focusSearchBox" keycode="VK_F4" modifiers=""
                command="cmd_focusSearchBox"/>
</keyset>

<popupset id="mainPopupSet">
    <popup id="fireinputIMEContainer" ignorekeys="true" onpopupshowing="Fireinput.IMEWindowShowing()" onpopuphidden="Fireinput.IMEWindowHidden();" onpopupshown="Fireinput.IMEWindowShown();">
      <vbox id="fireinputIMEContainerBox" style="padding: 1px; margin:0px; background-color: lightgrey">
        <hbox style="margin: 1px 0px; padding:0px;">
         <hbox id="fireinputComposeField" style="margin-right:1px;">
         </hbox>
         <textbox id="fireinputField" rows="1" flex="1" value=""
           onmousedown="FireinputIMEPanel.IMEInputFieldMouseEvent(event)" 
           onfocus="FireinputIMEPanel.IMEInputFieldFocusEvent(event)" />
         <image style="cursor:pointer" src="chrome://fireinput/skin/search.png" tooltiptext="Alt+Enter" onclick="FireinputWebSearch.load()"/>
        </hbox>
        <hbox style="margin: 1px 0px; padding:0px; border-bottom: 1px solid #AEA48E;"/>
        <hbox id="fireinputWordList" style="margin: 0px; padding:0;min-width:500px">
           <hbox id="fireinputIMEList" style="margin-top:2px;" flex="10">
           </hbox>
           <toolbarbutton id="fireinputPrevSelButton" tooltiptext=""
                       oncommand="FireinputIMEPanel.prevSel();" disabled="true"/>
           <toolbarbutton id="fireinputNextSelButton" tooltiptext=""
                       oncommand="FireinputIMEPanel.nextSel();" disabled="true"/>
        </hbox>
        <vbox id="fireinputLongPanel" style="display: none">
           <menuseparator style="margin: 0px; padding: 0px"/>
        </vbox>
      </vbox>
   </popup>
</popupset>
    
<popup id="contentAreaContextMenu">
  <menuseparator/>
      <menuitem
        id="fireinputContextEnableIME"
        label=""
        oncommand="Fireinput.toggleFireinput(true);"
      />
      <menuitem
        id="fireinputContextSelectImage"
        label=""
        oncommand="FireinputEmotions.addContextSelectedImage();"
      />
      <menuitem
        id="fireinputContextEnhanceWordTable"
        label=""
        oncommand="FireinputLongTable.addSelectionIntoTable();"
      />
      <menu id="fireinputContextSwitchEncoding" label="">
